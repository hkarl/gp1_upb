[   {   'blaetter': [0, 0, 5, 11, 16, 12, 29, 17, 19, 3, 5, 3, 9, 2],
        'kind': 'PUE'},
    {'blaetter': [5, 15, 12, 22, 19, 29, 7, 6, 2, 3, 5, 4, 2], 'kind': 'HUE'}]
